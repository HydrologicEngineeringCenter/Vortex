<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="calculator.controller.RasterController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Operation:" />
      <ComboBox fx:id="operationComboBox" prefWidth="150.0" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Raster:">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Text>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" VBox.vgrow="ALWAYS">
         <children>
            <TextField fx:id="rasterDataSource" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin></TextField>
            <Button fx:id="browse" maxHeight="25.0" maxWidth="25.0" minHeight="25.0" minWidth="25.0" mnemonicParsing="false" onAction="#handleBrowse" prefHeight="25.0" prefWidth="25.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin></Button>
         </children>
      </HBox>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Resampling method:" />
      <ComboBox fx:id="resamplingMethod" prefWidth="150.0" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
