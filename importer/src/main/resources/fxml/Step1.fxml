<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox spacing="10.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="importer.controller.Step1Controller">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Add files:">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Text>
      <HBox spacing="10.0" VBox.vgrow="ALWAYS">
         <children>
            <ListView fx:id="inFiles" HBox.hgrow="ALWAYS" />
            <VBox spacing="10.0">
               <children>
                  <Button fx:id="add" maxHeight="25.0" maxWidth="25.0" minHeight="25.0" minWidth="25.0" mnemonicParsing="false" onAction="#handleAdd" prefHeight="25.0" prefWidth="25.0" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
