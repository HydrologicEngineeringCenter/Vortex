<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox fx:id="dssUnitsOverride" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sanitizer.controller.DssUnitsOverrideController">
   <children>
      <CheckBox fx:id="overrideDssUnitsCheckBox" mnemonicParsing="false" text="Override DSS units" />
      <HBox fx:id="unitsStringBox" spacing="10.0">
         <children>
            <Pane prefWidth="12.0" />
            <Text fx:id="unitsStringLabel" strokeType="OUTSIDE" strokeWidth="0.0" text="Units string:" />
            <TextField fx:id="unitsString" />
         </children>
      </HBox>
   </children>
</VBox>
